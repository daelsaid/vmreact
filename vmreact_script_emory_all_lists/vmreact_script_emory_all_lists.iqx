******************************
No Cue Condition
******************************

Note:
* presents target and flankers in the predetermined positions and with the predetermined directions
(depending on flanker congruency, target position, and target direction)
* trial timings are set up as if a cue was presented even in the no-cue condition
* takes in responses up to predetermined duration (parameters.target_duration) after onset of target
* determines accuracy and validity of response
* updates counts and sums of reaction times (if correct and valid response)
* determines the duration of trial.TC_isi
<trial nocue>
/ ontrialbegin = [
	values.cuecondition = 1;
	values.trialcount += 1;
	values.trialcount_nocue += 1;
	values.cuevalidity = "NA";
	values.correct = 0;
	values.CT_IsI = list.CueTargetISI.nextvalue;
	values.TC_ISI= list.TargetCueIsI.nextvalue;
	values.flankercongruence= list.FlankerCongruence.nextvalue;
	values.targetposition= list.Targetlocation.nextvalue;
	values.targetdirection= list.Targetdirection.nextvalue;
	values.locationcongruence= list.LocationCongruence.nextvalue;
	values.cue1_x = 50%;
	values.cue1_y = 50%;
	values.target_y = 50%;
	values.flanker1_y = values.target_y;
	values.flanker2_y = values.target_y;
	values.flanker3_y = values.target_y;
	values.flanker4_y = values.target_y;
	if (values.targetposition == 2) {
		values.target_x = parameters.left_x;
		values.flanker1_x = parameters.left_x - 2*parameters.flankerdistance;
		values.flanker2_x = parameters.left_x - parameters.flankerdistance;
		values.flanker3_x = parameters.left_x + parameters.flankerdistance;
		values.flanker4_x = parameters.left_x + 2* parameters.flankerdistance;
	} else {
		values.target_x = parameters.right_x;
		values.flanker1_x = parameters.right_x - 2*parameters.flankerdistance;
		values.flanker2_x = parameters.right_x - parameters.flankerdistance;
		values.flanker3_x = parameters.right_x + parameters.flankerdistance;
		values.flanker4_x = parameters.right_x + 2* parameters.flankerdistance;
	};
	if (values.targetdirection == 1)
		values.selecttarget = 1;
	else
		values.selecttarget = 2;
	if (values.flankercongruence == 1)
		values.selectflanker = values.selecttarget;
	else {
		if (values.selecttarget == 1)
			values.selectflanker = 2;
		else
			values.selectflanker = 1;
	};
	trial.nocue.insertstimulustime(text.fixation, parameters.cue_duration);
	trial.nocue.insertstimulustime(picture.target, (parameters.cue_duration+values.CT_IsI));
	trial.nocue.insertstimulustime(picture.flanker1, (parameters.cue_duration+values.CT_IsI));
	trial.nocue.insertstimulustime(picture.flanker2, (parameters.cue_duration+values.CT_IsI));
	trial.nocue.insertstimulustime(picture.flanker3, (parameters.cue_duration+values.CT_IsI));
	trial.nocue.insertstimulustime(picture.flanker4, (parameters.cue_duration+values.CT_IsI));
	trial.nocue.insertstimulustime(shape.innerbox_left, (parameters.cue_duration + values.CT_ISI + parameters.target_duration));
	trial.nocue.insertstimulustime(shape.innerbox_right, (parameters.cue_duration + values.CT_ISI + parameters.target_duration));
]
/ stimulustimes = [0=fixation, outerbox_right, innerbox_right, outerbox_left, innerbox_left]
/ beginresponsetime = parameters.cue_duration+values.CT_IsI
/ response = timeout(parameters.responsewindow)
/ responseinterrupt = frames
/ validresponse = (parameters.responsekey_left, parameters.responsekey_right)
/ iscorrectresponse = [(values.targetdirection == 1 && trial.nocue.response == parameters.responsekey_right) || (values.targetdirection == 2 && trial.nocue.response ==parameters.responsekey_left)]
/ ontrialend = [
	trial.nocue.resetstimulusframes();
	values.rt = trial.nocue.latency;
	if (values.rt >= parameters.min_validlatency)
		values.valid = 1;
	else
		values.valid = 0;
	if (trial.nocue.correct && values.valid == 1) {
		values.correct = 1;
		list.accuracy.insertitem(values.correct, 1);
		list.latencies.insertitem(trial.nocue.latency, 1);
		values.countcorrect += 1;
		values.sumrt += values.rt;
		values.sstotal += (values.rt * values.rt);
	} else {
		list.accuracy.insertitem(0, 1);
	};
	if (values.rt <= parameters.target_duration)
		values.isitrial_duration = values.TC_ISI;
	else
		values.isitrial_duration = values.TC_ISI - (values.rt - parameters.target_duration);
]
/ recorddata = true
/ branch = [trial.TC_ISI]
</trial>

